import java.util.*;
class  Tablewithoutop
{
	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		System.out.print("The char is:");
		char st = s.next().toUpperCase().charAt(0);
		s.nextLine();
		int count = 1;
		for(char i='A';i<=st;i++){
			if(i==st) break;
			count++;
		}
		int res=0;
		for(int i=1;i<=10;i++){
			System.out.println(count+ " * "+i+" = "+res+=count );
		}
	}
}

 