class Bike 
{
	String name;
	double price;
	
	public void race(){
		System.out.println("1000 horse speed bike");
	}
	public void collect(Bike b){
		System.out.println("Bike address: "+b);
	}
	public static void main(String[] args) 
	{
		Bike b1 = new Bike();
		b1.name = "Yamaha-R15 (V1)";
		b1.price = 120000;
		System.out.println(b1.name);
		System.out.println(b1.price);
		b1.race();
		b1.collect(b1);
		System.out.println("_________________________________________________");
		Bike b2 = new Bike();
		b1.name = "Royal enfield gt 650 continental";
		b1.price = 450000;
		System.out.println(b2.name);
		System.out.println(b2.price);
		b2.race();
		b2.collect(b2);
	}
	//Multiline Non static Initializers
	{
		System.out.println("Welcome to KTM");
	}
}
